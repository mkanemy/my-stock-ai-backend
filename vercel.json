{
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "build/src/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["build/api/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/api/index.js"
        }
    ]
}